<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:orientation="vertical"
        android:padding="5dp">

        <ImageView
            android:id="@+id/dungeon_type_img"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:src="@drawable/ic_dungeon_type_1_p"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/element_type_img"
            android:layout_width="22.5dp"
            android:layout_height="22.5dp"
            android:src="@drawable/ic_element_1_p"
            app:layout_constraintBottom_toBottomOf="@+id/dungeon_type_img"
            app:layout_constraintLeft_toRightOf="@+id/dungeon_type_img"
            app:layout_constraintTop_toTopOf="@+id/dungeon_type_img" />

        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0000000000"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="@+id/dungeon_type_img"
            app:layout_constraintLeft_toRightOf="@+id/element_type_img"
            app:layout_constraintTop_toTopOf="@+id/dungeon_type_img" />

        <ImageView
            android:id="@+id/party_image"
            android:layout_width="266dp"
            android:layout_height="68dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/dungeon_type_img" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>